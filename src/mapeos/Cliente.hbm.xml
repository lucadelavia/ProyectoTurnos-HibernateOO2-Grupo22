<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping SYSTEM "../hibernate-mapping-3.0.dtd">

<hibernate-mapping package="datos"> 
    
  <joined-subclass name="Cliente"
                   extends="Usuario"
                   table="clientes">
                   
    <key column="idUsuario"/>
    
    <property name="nroCliente" column="nroCliente" type="integer"/>

  </joined-subclass>
</hibernate-mapping>